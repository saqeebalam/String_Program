//WAP to count number of closing and opening bracket() if same than return no error else give error
public class Count_Number_OpeningandCloseingBracket {
	public static void main(String[] args) {
		String str="(((())))";
		int count=0;
		int count1=0;
		for(int i=0;i<str.length();i++) {
			if(str.charAt(i)=='(') 
				count++;
			else if(str.charAt(i)==')')
				count1++;
		}
		if(count==count1) 
			System.out.println("No Error");
		else
			System.out.println("Error");
	}

}
