
public class String_Reverse {
	public static void main(String[] args) {
		String name=" Saqeeb Alam ";//here we take string
		for(int i=name.length()-1;i>=0;i--) {//here we take length of string-1(for resolve ArrayIndexOfBound Exception)
			System.out.print(name.charAt(i));//variale.charAt(IndexValue)print character one by one
		}
	}

}
